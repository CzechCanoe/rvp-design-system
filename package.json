{
  "name": "@czechcanoe/rvp-design-system",
  "version": "1.0.3",
  "description": "Design system pro registracni a vysledkovy servis Ceskeho svazu kanoistu",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/rvp-design-system.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:quick": "playwright test tests/prototypes.spec.ts --project=chromium",
    "test:proto": "playwright test tests/prototypes.spec.ts",
    "test:components": "playwright test tests/components.spec.ts --project=chromium",
    "test:update": "playwright test --update-snapshots",
    "test:update:quick": "playwright test tests/prototypes.spec.ts --project=chromium --update-snapshots",
    "test:report": "playwright show-report"
  },
  "keywords": [
    "design-system",
    "react",
    "typescript",
    "csk",
    "canoe"
  ],
  "author": "Czech Canoe",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CzechCanoe/rvp-design-system.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^17.0.0",
    "playwright": "^1.57.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^8.5.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "dependencies": {
    "lucide-react": "^0.563.0"
  }
}
